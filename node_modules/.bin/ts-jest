#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/chocobo/Documents/dev/practices/node-js/node-ts-global/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_a12c6e7dde7da368512487ea1887b242/node_modules/ts-jest/node_modules:/home/chocobo/Documents/dev/practices/node-js/node-ts-global/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_a12c6e7dde7da368512487ea1887b242/node_modules:/home/chocobo/Documents/dev/practices/node-js/node-ts-global/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/chocobo/Documents/dev/practices/node-js/node-ts-global/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_a12c6e7dde7da368512487ea1887b242/node_modules/ts-jest/node_modules:/home/chocobo/Documents/dev/practices/node-js/node-ts-global/node_modules/.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_a12c6e7dde7da368512487ea1887b242/node_modules:/home/chocobo/Documents/dev/practices/node-js/node-ts-global/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_a12c6e7dde7da368512487ea1887b242/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-jest@29.4.0_@babel+core@7.28.0_@jest+transform@29.7.0_@jest+types@29.6.3_babel-jest@_a12c6e7dde7da368512487ea1887b242/node_modules/ts-jest/cli.js" "$@"
fi
